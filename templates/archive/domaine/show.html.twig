{% extends 'base.html.twig' %}

{% block title %}Domaine{% endblock %}

{% block body %}
    <h1>Domaine</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ domaine.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ domaine.code }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ domaine.libelle }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ domaine.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ domaine.slug }}</td>
            </tr>
            <tr>
                <th>PubliePar</th>
                <td>{{ domaine.publiePar }}</td>
            </tr>
            <tr>
                <th>ModifiePar</th>
                <td>{{ domaine.modifiePar }}</td>
            </tr>
            <tr>
                <th>PublieLe</th>
                <td>{{ domaine.publieLe ? domaine.publieLe|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ModifieLe</th>
                <td>{{ domaine.modifieLe ? domaine.modifieLe|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_archive_domaine_index') }}">back to list</a>

    <a href="{{ path('app_archive_domaine_edit', {'id': domaine.id}) }}">edit</a>

    {{ include('archive/domaine/_delete_form.html.twig') }}
{% endblock %}
