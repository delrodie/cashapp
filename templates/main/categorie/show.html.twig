{% extends 'base.html.twig' %}

{% block title %}Categorie{% endblock %}

{% block body %}
    <h1>Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categorie.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ categorie.code }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ categorie.libelle }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ categorie.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_main_categorie_index') }}">back to list</a>

    <a href="{{ path('app_main_categorie_edit', {'id': categorie.id}) }}">edit</a>

    {{ include('main/categorie/_delete_form.html.twig') }}
{% endblock %}
